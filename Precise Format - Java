class Solution {
    static ArrayList<Float> divisionWithPrecision(float a, float b) {
        
        
        ArrayList<Float> result = new ArrayList<>();
        
        float value = a / b;
        
        float rounded = Math.round(value*1000f)/1000f;
        
        result.add(value);
        result.add(rounded);
        
        return result;
        
    }
}
